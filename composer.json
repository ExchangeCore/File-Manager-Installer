{
  "require": {
    "exchangecore/file-manager": "dev-master"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/ExchangeCore/File-Manager.git"
    }
  ],
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*"
    },
  "minimum-stability": "dev",
    "autoload": {
        "psr-4": {"application\\": "application"}
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}